<h1>Articles</h1>

<% @articles.each do |article| %>
  <h2><%= article.title %></h2>
    <table>
      <tr>
        <td><%= image_tag("#{article.image}", :class =>"recipeimages") %></td>
        <td><%= article.text %></br></td>
      </tr>
    </table>
  <h2>Quote: <%= article.quote %></h2></br>
  <% end %>
